{
  "name": "www",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "style": "sass public/stylesheets/customBootstrap.scss | postcss --use autoprefixer -o public/stylesheets/customBootstrap.css",
    "style-min": "sass public/stylesheets/customBootstrap.scss --style compressed | postcss --use autoprefixer -o public/stylesheets/customBootstrap.min.css",
    "style-all": "npm-run-all --parallel style style-min"
  },
  "dependencies": {
    "autoprefixer": "^10.2.5",
    "bigbluebutton-js": "^0.2.0",
    "bootstrap": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsqr": "^1.4.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.5",
    "morgan": "~1.9.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  }
}
